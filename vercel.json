{
  "version": 2,
  "routes": [
    {
      "src": "/_expo/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/dist/_expo/static/$1"
    },
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/dist/static/$1"
    },
    {
      "src": "/favicon/(.*)",
      "dest": "/dist/favicon/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/dist/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
