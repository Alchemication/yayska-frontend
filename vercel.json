{
  "version": 2,
  "rewrites": [
    {
      "source": "/_expo/static/js/web/(.*).js",
      "destination": "/dist/_expo/static/js/web/$1.js"
    },
    { "source": "/static/(.*)", "destination": "/dist/static/$1" },
    { "source": "/auth/google/callback", "destination": "/dist/index.html" },
    {
      "source": "/auth/google/callback/:path*",
      "destination": "/dist/index.html"
    },
    { "source": "/(.*)", "destination": "/dist/index.html" }
  ]
}
